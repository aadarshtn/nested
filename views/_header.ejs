<header id = "website-header">
    <div id = "website-logo">
        <p>NESTED</p>
    </div>
    <ul id = "header-ul">
        <% if (locals.user) { %>
            <div class = "user-actions">
                <li>
                    <a href="/users/profile">
                        <%= user.name %> |
                    </a>
                </li>
                <li>
                    <a href="/users/sign-out">| Sign Out</a>
                </li>
            </div>
        <% } else { %>
            <div class = "user-actions">
                <li><a href="/users/sign-in">Sign In |</a></li>
                <li><a href="/users/sign-up">| Sign Up</a></li>
            </div>
        <% } %>
    </ul>
</header>